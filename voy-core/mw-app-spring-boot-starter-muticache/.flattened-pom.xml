<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>cn.com.mw</groupId>
  <artifactId>mw-app-spring-boot-starter-muticache</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <name>mw-app-spring-boot-starter-muticache</name>
  <description>多级缓存套件</description>
  <distributionManagement>
    <repository>
      <id>shuchan-releases</id>
      <url>http://10.0.1.224:11081/repository/shuchan-releases/</url>
    </repository>
    <snapshotRepository>
      <id>shuchan-snapshots</id>
      <url>http://10.0.1.224:11081/repository/shuchan-snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <dependencies>
    <dependency>
      <groupId>cn.com.mw</groupId>
      <artifactId>mw-app-common</artifactId>
      <version>1.0.0-SNAPSHOT</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.alicp.jetcache</groupId>
      <artifactId>jetcache-starter-redis-lettuce</artifactId>
      <version>2.7.0</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-data-redis</artifactId>
      <version>2.6.9</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.commons</groupId>
      <artifactId>commons-pool2</artifactId>
      <version>2.11.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.springframework.cloud</groupId>
      <artifactId>spring-cloud-context</artifactId>
      <version>3.1.1</version>
      <scope>compile</scope>
      <optional>true</optional>
    </dependency>
  </dependencies>
  <repositories>
    <repository>
      <id>nexus-shuchan</id>
      <name>nexus-shuchan</name>
      <url>http://10.0.1.224:11081/repository/shuchan-public/</url>
    </repository>
  </repositories>
</project>
